{
  "name": "ztree-for-vue",
  "version": "1.2.2",
  "description": "A Vue.2x component of ztree",
  "private": false,
  "main": "src/lib/vue-ztree.vue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "hhttps://github.com/arixse/vue-ztree.git"
  },
  "bug": {
    "url": "https://github.com/arixse/vue-ztree/issues"
  },
  "keywords": [
    "vue",
    "ztree",
    "jquery ztree",
    "vue-ztree",
    "vue ztree",
    "vue component",
    "ztree-for-vue"
  ],
  "author": "arixse",
  "dependencies": {
    "vue": "^2.6.10"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.2.3",
    "jest": "^25.2.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.0.1",
    "ztree-for-vue": "^1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "vue-eslint-parser",
      "sourceType": "module"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/lib/**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
